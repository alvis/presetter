{
  "name": "presetter-preset-next",
  "version": "8.0.6",
  "description": "An opinionated presetter preset for a Next JS project",
  "keywords": [
    "presetter",
    "preset"
  ],
  "homepage": "https://github.com/alvis/presetter#readme",
  "bugs": {
    "url": "https://github.com/alvis/presetter/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alvis/presetter.git"
  },
  "license": "MIT",
  "author": "Alvis HT Tang <alvis@hilbert.space>",
  "sideEffects": false,
  "type": "module",
  "imports": {
    "#*": {
      "types": "./src/*.ts",
      "development": "./src/*.ts",
      "import": "./lib/*.js"
    }
  },
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "import": "./lib/index.js"
    }
  },
  "scripts": {
    "build": "run build",
    "coverage": "run coverage --",
    "lint": "run lint --",
    "prepublishOnly": "run prepare && run prepublishOnly",
    "test": "run test --",
    "watch": "run watch --"
  },
  "dependencies": {
    "presetter-preset-esm": "workspace:*",
    "presetter-preset-react": "workspace:*",
    "presetter-preset-web": "workspace:*",
    "presetter-types": "workspace:*",
    "type-fest": "catalog:"
  },
  "peerDependencies": {
    "@next/eslint-plugin-next": "^15.0.0",
    "@storybook/nextjs-vite": "^9.0.0",
    "@tailwindcss/postcss": "^4.0.0",
    "next": "^15.0.0",
    "postcss-preset-env": "^10.0.0",
    "presetter": "workspace:*",
    "presetter-preset-esm": "workspace:*",
    "presetter-preset-react": "workspace:*",
    "presetter-preset-strict": "workspace:*",
    "presetter-preset-web": "workspace:*",
    "sharp": ">=0.34.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./lib/index.d.ts",
        "import": "./lib/index.js"
      }
    },
    "imports": {
      "#*": {
        "types": "./lib/*.d.ts",
        "import": "./lib/*.js"
      }
    }
  }
}
