{
  "ci": {
    "collect": {
      "staticDistDir": "./build",
      "numberOfRuns": 3,
      "url": [
        "http://localhost/presetter/",
        "http://localhost/presetter/docs/",
        "http://localhost/presetter/docs/tutorials/",
        "http://localhost/presetter/docs/api/",
        "http://localhost/presetter/docs/presets/"
      ]
    },
    "assert": {
      "assertions": {
        "categories:performance": ["warn", {"minScore": 0.8}],
        "categories:accessibility": ["error", {"minScore": 0.95}],
        "categories:best-practices": ["warn", {"minScore": 0.9}],
        "categories:seo": ["error", {"minScore": 0.9}],
        "categories:pwa": ["warn", {"minScore": 0.8}]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}